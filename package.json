{
  "name": "ng2-feeder",
  "version": "0.0.1",
  "main": "lib/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -w 'prod/**/*.*' ./prod/server.js",
    "up": "concurrent --kill-others \"npm run babel:w\" \"npm run start\" \"webpack -w\"",
    "babel": "babel src --out-dir prod",
    "babel:w": "babel -w src --out-dir prod",
    "js": "./node_modules/.bin/webpack --progress --colors --config webpack.config.js --bail",
    "devserver": "webpack-dev-server --json --progress",
    "dumpdev": "rm -rf www/* && webpack --progress --colors -d",
    "dumpprod": "rm -rf www/* && webpack --progress --colors --config webpack.config.prod.js -p"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "async": "^1.5.2",
    "bcryptjs": "^2.3.0",
    "body-parser": "latest",
    "connect-mongo": "^1.1.0",
    "core-js": "~2.4.0",
    "cropperjs": "^0.7.2",
    "csv": "^1.1.0",
    "express": "^4.x",
    "express-jwt": "^3.4.0",
    "jquery": "^3.1.0",
    "jsonwebtoken": "^7.1.6",
    "lodash": "^4.9.0",
    "method-override": "latest",
    "mongoose": "latest",
    "morgan": "latest",
    "multiparty": "^4.1.2",
    "mv": "^2.1.1",
    "nconf": "^0.8.4",
    "ng2-file-upload": "^1.0.3",
    "node-csv": "^0.1.2",
    "node-sass": "^3.4.1",
    "node-uuid": "^1.4.7",
    "redis": "^2.5.3",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "socket.io": "^1.4.5",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "angular-cli": "*",
    "autoprefixer": "~6.3.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "concurrently": "^2.2.0",
    "cross-env": "~1.0.8",
    "css-loader": "*",
    "file-loader": "*",
    "file-stream-rotator": "*",
    "html-loader": "*",
    "html-webpack-plugin": "*",
    "json-loader": "*",
    "nodemon": "^1.9.1",
    "npm-run-all": "~2.3.0",
    "path": "^0.4.9",
    "postcss-loader": "*",
    "raw-loader": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "ts-loader": "*",
    "typescript": "*",
    "typings": "~1.3.1",
    "util": "^0.10.3",
    "webpack": "*",
    "webpack-dev-server": "*",
    "webpack-md5-hash": "~0.0.5",
    "winston": "^2.2.0"
  }
}
